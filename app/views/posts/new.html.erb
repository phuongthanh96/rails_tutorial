<h1>This is new post </h1>
<%= form_for @post do |f| %>
 <%= render "shared/error_messages", object: f.object %>
  <div class="form-group">
	<%= f.label :title %>
	<%= f.text_field :title, class:"form-control" ,placeholder: "Enter your title" %>
  </div>

  <div class="form-group">
	<%= f.label :content %>
	<%= f.text_area :content,  class:"form-control" ,placeholder: "Enter your content" %>
  </div>

  <div class="form-group">
  <%= f.file_field :image_url %>
  </div>
  <%= f.submit :save , class: "btn btn-primary" %>
<% end %>